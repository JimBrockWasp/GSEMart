#tag Events cmdDelete
	#tag Event
		Sub Action()
		  ' ----------------------------------------------------------------------
		  ' Procedure Name: cmdDelete.Action
		  ' Date          :
		  ' By            : Jim Brock
		  ' Notes
		  '
		  ' ----------------------------------------------------------------------
		  Try
		    
		    ' Connect to the database
		    Dim db As New MSSQLServerDatabaseEx
		    Dim sp As New SQLStoredProcedure(db, "DLDeleteIndex")
		    Dim rs As RecordSet
		    Dim FileName As String
		    
		    
		    sp.AddParameter ("@IndexId", CInt(lstDocuments.RowTag(lstDocuments.ListIndex)))
		    rs = sp.ExecuteRecordSet
		    
		    ' Get the file to delete
		    FileName = rs.Field("FullFilePath").StringValue.DefineEncoding(Encodings.WindowsANSI)
		    
		    ' Delete the file
		    Dim ThisFile As FolderItem
		    ThisFile = GetFolderItem(FileName)
		    ThisFile.Delete
		    
		    
		    ' Disconnect
		    db.Close
		    
		    LoadDocuments
		    
		    
		  Catch Err as RunTimeException
		    Session.DisplayError "cmdDelete.Action", Err
		    
		  End Try
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events tmrSetListIndex
	#tag Event
		Sub Action()
		  Me.Mode = timer.ModeOff
		  LoadDocuments
		End Sub
	#tag EndEvent
#tag EndEvents
