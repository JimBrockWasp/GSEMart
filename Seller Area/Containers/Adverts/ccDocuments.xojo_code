#tag Events cmdUpload
	#tag Event
		Sub Action()
		  ' ----------------------------------------------------------------------
		  ' Procedure Name: cmdUpload.Action
		  ' Date          :
		  ' By            : Jim Brock
		  ' Notes
		  ' Upload the logo to the database
		  ' ----------------------------------------------------------------------
		  Try
		    
		    If upDocuments.FileCount > 0 and txtDocumentTitle.Text <> "" Then
		      upDocuments.Upload
		    Else
		      
		      Dim Message() As String
		      Message.Append("You must select a file and give it a title")
		      Message.Append("")
		      Message.Append("Click the '+' sign to select a file")
		      
		      
		      Dim dlg As New dlgErrorMessage
		      dlg.lblHeading.Text = "Invalid Entry"
		      dlg.lblMessage.Text = Join(Message, EndOfLine)
		      dlg.Show
		      
		    End If
		    
		  Catch Err as RunTimeException
		    session.DisplayError "cmdUpload.Action", Err
		    
		  End Try
		End Sub
	#tag EndEvent
