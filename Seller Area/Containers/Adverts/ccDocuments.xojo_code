#tag Events lstDocuments
	#tag Event
		Sub CellClick(Row As Integer, Column As Integer)
		  ' ----------------------------------------------------------------------
		  ' Procedure Name: ccDocuments.lstDocuments.CellClick
		  ' Date          :
		  ' By            : Jim Brock
		  ' Notes
		  '
		  ' ----------------------------------------------------------------------
		  Try
		    
		    Dim CurrentColumn As Integer
		    Dim AdvertDocumentId As Integer
		    
		    If Column = 1 Then
		      
		      AdvertDocumentId = CInt(lstDocuments.Cell(Row, 0))
		      SetDocumentAsDefault (AdvertDocumentId)
		      
		      ' Set the timer off to reload the Documents
		      tmrSetListIndex.Mode = timer.ModeSingle
		      
		    End If
		    
		  Catch Err as RunTimeException
		    session.DisplayError "ccDocuments.lstDocuments.CellClick", Err
		    
		  End Try
		End Sub
