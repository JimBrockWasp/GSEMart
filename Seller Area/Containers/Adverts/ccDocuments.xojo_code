#tag WindowCode
	#tag Event
		Sub Shown()
		  ' ----------------------------------------------------------------------
		  ' Procedure Name: ccDocuments.Shown
		  ' Date          :
		  ' By            : Jim Brock
		  ' Notes
		  '
		  ' ----------------------------------------------------------------------
		  Try
		    
		    lstDocuments.Heading(0) = "Title"
		    
		  Catch Err as RunTimeException
		    session.DisplayError "ccDocuments.Shown", Err
		    
		  End Try
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub LoadDocuments()
		  ' ----------------------------------------------------------------------
		  ' Procedure Name: ccDocuments.LoadDocuments
		  ' Date          :
		  ' By            : Jim Brock
		  ' Notes
		  '
		  ' ----------------------------------------------------------------------
		  Try
		    
		    ' Connect to the database
		    Dim db As New MSSQLServerDatabaseEx
		    Dim sp As New SQLStoredProcedure(db, "DLGetDocumentsByAdvertId")
		    Dim rs As RecordSet
		    
		    sp.AddParameter("@AdvertId", AdvertId)
		    rs = sp.ExecuteRecordSet
		    
		    lstDocuments.DeleteAllRows
		    
		    If Not rs.EOF Then
		      
		      PopulateDocumentList (rs)
		      cmdDelete.Enabled = True
		      lstDocuments.ListIndex = 0
		      
		    Else
		      
		      cmdDelete.Enabled = False
		      
		    End If
		    
		    rs.Close
		    
		    ' Disconnect
		    db.Close
		    
		    
		    
		  Catch Err as RunTimeException
		    Session.DisplayError "ccDocuments.LoadDocuments", Err
		    
		  End Try
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub PopulateDocumentList(rs As RecordSet)
		  ' ----------------------------------------------------------------------
		  ' Procedure Name: ccDocuments.PopulateDocumentList
		  ' By            : Jim Brock
		  ' ----------------------------------------------------------------------
		  Try
		    
		    Dim Col as integer
		    Dim CellValue As String
		    
		    ' Clear the list
		    lstDocuments.deleteAllRows
		    
		    ' Loop until we reach the end of the recordset
		    While Not rs.eof
		      
		      ' Add a new row
		      lstDocuments.addRow ""
		      
		      lstDocuments.Cell(lstDocuments.lastIndex, 0 ) = rs.Field("Title").StringValue.DefineEncoding(Encodings.WindowsANSI)
		      
		      ' Save the first column as the Id in the RowTag
		      lstDocuments.RowTag (lstDocuments.LastIndex) = rs.Field("IndexId").IntegerValue
		      
		      rs.moveNext// move to the next record
		    Wend
		    
		    
		  Catch Err as RunTimeException
		    session.DisplayError "ccDocuments.PopulateDocumentList", Err
		    
		  End Try
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h1000
		Sub PublishDocument(ThisWebFile As WebUploadedFile)
		  ' ----------------------------------------------------------------------
		  ' Procedure Name: frmFileMonitor.PublishDocument
		  ' Date          : 2014-08-26
		  ' By            : Jim Brock
		  ' Notes
		  ' Publish the selected document
		  ' ----------------------------------------------------------------------
		  
		  ' Create a new datanode
		  Dim ThisDataNode As New DataNode
		  
		  ' Create a new document Index
		  Dim ThisIndex As New DocumentIndex (ThisDataNode)
		  
		  Try
		    
		    ' Begin a transaction
		    ThisIndex.BeginTransaction
		    
		    ThisIndex.AdvertId = AdvertId
		    ThisIndex.SourceFile = ThisWebFile
		    ThisIndex.DocumentTitle = txtDocumentTitle.Text
		    
		    ThisIndex.MoveToDataNode
		    
		    ' Write a record to the index
		    ThisIndex.InsertIndex
		    
		    ' Commit the transaction
		    ThisIndex.Commit
		    
		    ' Clear the document Title field
		    txtTitle.Text = ""
		    
		    
		  Catch PublishErr As RunTimeException
		    
		    ' Rollback the transaction 
		    ThisIndex.RollBack
		    Session.DisplayError "ccDocuments.PublishDocument", PublishErr
		    
		    
		    
		  End Try
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub SetDocumentAsDefault(AdvertDocumentId As Integer)
		  ' ----------------------------------------------------------------------
		  ' Procedure Name: ccDocuments.SetDocumentAsDefault
		  ' Date          :
		  ' By            : Jim Brock
		  ' Notes
		  '
		  ' ----------------------------------------------------------------------
		  Try
		    
		    ' Connect to the database
		    Dim db As New MSSQLServerDatabaseEx
		    Dim sp As New SQLStoredProcedure(db, "slSetAdvertDocumentAsDefault")
		    
		    
		    sp.AddParameter("@AdvertId", AdvertId)
		    sp.AddParameter("@AdvertDocumentId", AdvertDocumentId)
		    sp.ExecuteNonRecordSet
		    
		    ' Disconnect
		    db.Close
		    
		    
		  Catch Err as RunTimeException
		    session.DisplayError "ccDocuments.SetDocumentAsDefault", Err
		    
		  End Try
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub Show(ThisAdvertId As Integer)
		  ' ----------------------------------------------------------------------
		  ' Procedure Name: ccDocuments.Show
		  ' Date          :
		  ' By            : Jim Brock
		  ' Notes
		  '
		  ' ----------------------------------------------------------------------
		  Try
		    
		    'Me.Top = 0
		    'Me.Left = 0
		    'Me.Width = Me.Parent.Width - 10
		    'Me.Height = Me.Parent.Height - 10
		    Me.Visible = True
		    
		    AdvertId = ThisAdvertId
		    txtAdvertId.Text = ccAdverts(Me.Parent).conAdvert.txtAdvertId.Text
		    txtGSEType.Text = ccAdverts(Me.Parent).conAdvert.txtGSEType.Text
		    txtTitle.Text = ccAdverts(Me.Parent).conAdvert.txtTitle.Text
		    
		    LoadDocuments
		    
		  Catch Err as RunTimeException
		    session.DisplayError "ccDocuments.Show", Err
		    
		  End Try
		  
		  
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		AdvertId As Integer
	#tag EndProperty


