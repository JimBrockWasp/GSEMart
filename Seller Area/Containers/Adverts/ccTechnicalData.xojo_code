		Sub Show(ThisAdvertId As Integer, ThisGSETypeId As Integer)
		  ' ----------------------------------------------------------------------
		  ' Procedure Name: ccTechnical.Show
		  ' Date          :
		  ' By            : Jim Brock
		  ' Notes
		  '
		  ' ----------------------------------------------------------------------
		  Try
		    
		    'Me.Top = 0
		    'Me.Left = 0
		    'Me.Width = Me.Parent.Width - 10 
		    'Me.Height = Me.Parent.Height - 10
		    Me.Visible = True
		    
		    AdvertId = ThisAdvertId
		    GSETypeId = ThisGSETypeId
		    
		    txtAdvertId.Text = ccAdverts(Me.Parent).conAdvert.txtAdvertId.Text
		    txtGSEType.Text = ccAdverts(Me.Parent).conAdvert.txtGSEType.Text
		    txtTitle.Text = ccAdverts(Me.Parent).conAdvert.txtTitle.Text
		    
		    LoadSpecForGSEType
		    LoadQtyForGSEType
		    
		    LoadAdvertSpecs
		    LoadAdvertQtys
		    
		  Catch Err as RunTimeException
		    session.DisplayError "ccTechnical.Show", Err
		    
		  End Try
		  
		End Sub
