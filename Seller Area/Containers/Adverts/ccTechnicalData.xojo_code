		  ' ----------------------------------------------------------------------
		  Try
		    
		    ' Connect to the database
		    Dim db As New MSSQLServerDatabaseEx
		    Dim sp As New SQLStoredProcedure(db, "prSelAdvertQtysByAdvertId")
		    Dim rs As RecordSet
		    Dim n As Integer
		    Dim m As Integer
		    
		    sp.AddParameter("@AdvertId", AdvertId)
		    rs = sp.ExecuteRecordSet
		    
		    ' Update the spec descriptions
		    While Not rs.EOF
		      
		      If Not IsNullOrEmpty(rs.Field("MetricValue").StringValue.DefineEncoding(Encodings.WindowsANSI)) Then
		        txtQty(n).Text = rs.Field("MetricValue").StringValue.DefineEncoding(Encodings.WindowsANSI)
		      Else
