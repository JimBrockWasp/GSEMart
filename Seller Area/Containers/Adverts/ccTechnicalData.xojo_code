		Sub LoadQtyForGSEType()
		  ' ----------------------------------------------------------------------
		  ' Procedure Name: ccTechnicalData.LoadQtyForGSEType
		  ' Date          :
		  ' By            : Jim Brock
		  ' Notes
		  '
		  ' ----------------------------------------------------------------------
		  Try
		    
		    ' Connect to the database
		    Dim db As New MSSQLServerDatabaseEx
		    Dim sp As New SQLStoredProcedure(db, "prSelQtysByGSETypeId")
		    Dim rs As RecordSet
		    Dim n As Integer
		    Dim m As Integer
		    Dim MetricConversion As Double
		    Dim ListIndex As Integer
		    
		    sp.AddParameter("@GSETypeId", GSETypeId)
		    rs = sp.ExecuteRecordSet
		    
		    ' Update the spec descriptions
		    While Not rs.EOF
		      
		      
		      lblQty(n).Visible = True
		      txtQty(n).Visible = True
		      cboQtyUnits(n).Visible = True
		      
		      lblQty(n).Text = rs.Field("Description").StringValue.DefineEncoding(Encodings.WindowsANSI)
		      lblQty(n).Tag = rs.Field("QuantityId").StringValue.DefineEncoding(Encodings.WindowsANSI)
		      
		      cboQtyUnits(n).RemoveAllRows
		      cboQtyUnits(n).AddRow (rs.Field("MetricUnitDescription").StringValue.DefineEncoding(Encodings.WindowsANSI))
		      
		      MetricConversion = 1
		      ListIndex = cboQtyUnits(n).ListCount - 1
		      cboQtyUnits(n).RowTag(ListIndex) = MetricConversion
		      
		      If Not IsNullOrEmpty(rs.Field("ImperialUnitDescription").StringValue.DefineEncoding(Encodings.WindowsANSI)) Then
		        cboQtyUnits(n).AddRow (rs.Field("ImperialUnitDescription").StringValue.DefineEncoding(Encodings.WindowsANSI))
		        
		        ListIndex = cboQtyUnits(n).ListCount - 1
		        MetricConversion = rs.Field("MetricConversionRatio").DoubleValue
		        
		        cboQtyUnits(n).RowTag(ListIndex) = MetricConversion
		      End If
		      
		      cboQtyUnits(n).ListIndex = 0
		      
		      rs.MoveNext
		      n = n + 1
		      
		    Wend
		    
		    ' Hide the other spec inputs
		    For m = n to 1
		      lblQty(m).Visible = False
		      lblQty(m).Tag = ""
		      txtQty(m).Visible = False
		      cboQtyUnits(m).Visible = False
		    Next
		    
		    ' Disconnect
		    db.Close
		    
		  Catch Err as RunTimeException
		    Session.DisplayError "ccTechnicalData.LoadQtyForGSEType", Err
		    
		  End Try
		  
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub LoadSpecForGSEType()
		  ' ----------------------------------------------------------------------
		  ' Procedure Name: ccTechnicalData.LoadData
		  ' Date          :
		  ' By            : Jim Brock
		  ' Notes
		  '
		  ' ----------------------------------------------------------------------
		  Try
		    
		    ' Connect to the database
		    Dim db As New MSSQLServerDatabaseEx
		    Dim sp As New SQLStoredProcedure(db, "prSelSpecsByGSETypeId")
		    Dim rs As RecordSet
		    Dim n As Integer
		    Dim m As Integer
		    
		    sp.AddParameter("@GSETypeId", GSETypeId)
		    rs = sp.ExecuteRecordSet
		    
		    ' Update the spec descriptions
		    While Not rs.EOF
		      
		      lblSpec(n).Visible = True
		      cboSpec(n).Visible = True
		      
		      lblSpec(n).Text = rs.Field("Description").StringValue.DefineEncoding(Encodings.WindowsANSI)
		      lblSpec(n).Tag = rs.Field("SpecificationId").StringValue.DefineEncoding(Encodings.WindowsANSI)
		      
		      ' Load in the options for this spec
		      GetSpecOptions (rs.Field("SpecificationId").StringValue.DefineEncoding(Encodings.WindowsANSI), cboSpec(n))
		      
		      if cboSpec(n).ListCount = 1 Then
		        cboSpec(n).ListIndex = 0
		      End If
		      
		      rs.MoveNext
		      n = n + 1
		      
		    Wend
		    
		    ' Hide the other spec inputs
		    For m = n to 2
		      lblSpec(m).Visible = False
		      lblSpec(m).Tag = ""
		      cboSpec(m).Visible = False
		    Next
		    
		    ' Disconnect
		    db.Close
		    
		  Catch Err as RunTimeException
		    Session.DisplayError "ccTechnicalData.LoadData", Err
		    
		  End Try
		  
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub Save()
		  ' ----------------------------------------------------------------------
		  ' Procedure Name: ccTechnicalData.Save
		  ' Date          :
		  ' By            : Jim Brock
		  ' Notes
		  '
		  ' ----------------------------------------------------------------------
		  Try
		    
		    ' Connect to the database
		    Dim db As New MSSQLServerDatabaseEx
		    Dim sp As New SQLStoredProcedure(db, "slUpdateAdvertGSESpecifications")
		    
		    sp.AddParameter("@AdvertId", AdvertId)
		    
		    If cboSpec(0).ListIndex > 0 And lblSpec(0).Tag <> "" Then
		      sp.AddParameter("@SpecId1",CStr(lblSpec(0).Tag))
		      sp.AddParameter("@SpecOptionId1", CStr(cboSpec(0).RowTag(cboSpec(0).ListIndex)))
		    End If
		    
		    If cboSpec(1).ListIndex > 0  And lblSpec(1).Tag <> "" Then
		      sp.AddParameter("@SpecId2",CStr(lblSpec(1).Tag))
		      sp.AddParameter("@SpecOptionId2", CStr(cboSpec(1).RowTag(cboSpec(1).ListIndex)))
		    End If
		    
		    If cboSpec(2).ListIndex > 0  And lblSpec(2).Tag <> "" Then
		      sp.AddParameter("@SpecId3",CStr(lblSpec(2).Tag))
		      sp.AddParameter("@SpecOptionId3", CStr(cboSpec(2).RowTag(cboSpec(2).ListIndex)))
		    End If
		    
		    sp.ExecuteNonRecordSet
		    sp = Nil
		    
		    
		    ' Qty Values
		    Dim MetricValue As Double
		    Dim EnteredValue As Integer
		    Dim MetricConversion As Double
		    Dim ListIndex As Integer
		    
		    sp = New SQLStoredProcedure(db, "[slUpdateAdvertGSEQtys]")
		    sp.AddParameter("@AdvertId", AdvertId)
		    
		    If IsInteger(txtQty(0).Text) And lblQty(0).Tag <> "" Then
		      
		      ' QtyId
		      sp.AddParameter("@QtyId1",CStr(lblQty(0).Tag))
		      
		      ' Get the metric value for the Qty Entered
		      ListIndex = cboQtyUnits(0).ListIndex
		      EnteredValue = CDouble(txtQty(0).Text)
		      MetricConversion = cboQtyUnits(0).RowTag(ListIndex)
		      MetricValue = EnteredValue / MetricConversion
		      sp.AddParameter("@MetricValue1", MetricValue)
		      
		    End If
		    
		    If IsInteger(txtQty(1).Text) And lblQty(1).Tag <> "" Then
		      
		      ' QtyId
		      sp.AddParameter("@QtyId2",CStr(lblQty(1).Tag))
		      
		      ' Get the metric value for the Qty Entered
		      ListIndex = cboQtyUnits(1).ListIndex
		      EnteredValue = CDouble(txtQty(1).Text)
		      MetricConversion = cboQtyUnits(1).RowTag(ListIndex)
		      MetricValue = EnteredValue / MetricConversion
		      sp.AddParameter("@MetricValue2", MetricValue)
		      
		    End If
		    
		    sp.ExecuteNonRecordSet
		    
		    ' Disconnect
		    db.Close
		    
		  Catch Err as RunTimeException
		    Session.DisplayError "ccTechnicalData.Save", Err
		    
		  End Try
		  
		  
		End Sub
	#tag EndMethod

