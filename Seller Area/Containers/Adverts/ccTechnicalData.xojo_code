		Sub GetSpecOptions(SpecId As String, cboThisSpec As WebPopupMenuEx)
		  ' ----------------------------------------------------------------------
		  ' Procedure Name: ccTechnicalData.GetSpecOptions
		  ' Date          :
		  ' By            : Jim Brock
		  ' Notes
		  '
		  ' ----------------------------------------------------------------------
		  Try
		    
		    ' Connect to the database
		    Dim db As New MSSQLServerDatabaseEx
		    Dim sp As New SQLStoredProcedure(db, "prSelSpecOptions")
		    Dim rs As RecordSet
		    
		    sp.AddParameter("@GSETypeId", GSETypeId)
		    sp.AddParameter("@SpecId", SpecId)
		    
		    rs = sp.ExecuteRecordSet
		    
		    cboThisSpec.RemoveAllRows
		    
		    ' Add a blank row so the user can select nothing
		    cboThisSpec.AddRow("")
		    
		    While NOT rs.EOF
		      
		      cboThisSpec.AddRow(rs.Field("Description").StringValue.DefineEncoding(Encodings.WindowsANSI))
		      cboThisSpec.RowTag(cboThisSpec.ListCount -1) = rs.Field("SpecificationOptionId").StringValue.DefineEncoding(Encodings.WindowsANSI)
		      
		      rs.MoveNext
		      
		    Wend
		    
		    
		    rs.Close
		    
		    ' Disconnect
		    db.Close
		    
		  Catch Err as RunTimeException
		    Session.DisplayError "ccTechnicalData.GetSpecOptions", Err
		    
		  End Try
		End Sub
	#tag EndMethod

