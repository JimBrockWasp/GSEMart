#tag Events cmdSave
	#tag Event
		Sub Action()
		  ' ----------------------------------------------------------------------
		  ' Procedure Name: cmdSave.Action
		  ' Date          :
		  ' By            : Jim Brock
		  ' Notes
		  '
		  ' ----------------------------------------------------------------------
		  
		  Dim Message() As String
		  
		  
		  ' Check some values
		  If chkShowPrice.Value = True Then
		    
		    If cboCurrency.ListIndex = - 1 Then
		      Message.Append("Please select a Currency")
		    End If
		    
		    If CInt(txtPrice.Text) = 0 Then
		      Message.Append("Please Enter A Valid Price")
		    End If
		    
		  End If
		  
		  If Message.Ubound > -1 Then
		    
		    Dim dlgMessage As New dlgErrorMessage
		    dlgMessage.lblHeading.Text = "Invalid Entry"
		    dlgMessage.lblMessage.Text = Join(Message, EndOfLine)
		    dlgMessage.Show
		    
		  Else
		    
		    ' Start the timer off
		    StartSavingAdvert
		    
		  End If
		End Sub
	#tag EndEvent
