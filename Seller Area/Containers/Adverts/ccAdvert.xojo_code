		Sub UpdateAdvertList()
		  ' ----------------------------------------------------------------------
		  ' Procedure Name: ccAdvert.UpdateAdvertList
		  ' Date          :
		  ' By            : Jim Brock
		  ' Notes
		  '
		  ' ----------------------------------------------------------------------
		  Try
		    
		    Dim ListIndex As Integer
		    
		    ListIndex = ccAdverts(Me.Parent).RowClicked
		    
		    ccAdverts(Self.Parent).lstAdverts.Cell(listIndex, 2 ) = txtTitle.Text
		    ccAdverts(Self.Parent).lstAdverts.CellStyle(listIndex, ccAdverts.Status_NotPlaced ) = styListCellNotSelected
		    ccAdverts(Self.Parent).lstAdverts.CellStyle(listIndex, ccAdverts.Status_Placed ) = styListCellNotSelected
		    ccAdverts(Self.Parent).lstAdverts.CellStyle(listIndex, ccAdverts.Status_Sold ) = styListCellNotSelected
		    ccAdverts(Self.Parent).lstAdverts.CellStyle(listIndex, ccAdverts.Status_Deleted ) = styListCellNotSelected
		    ccAdverts(Self.Parent).lstAdverts.CellStyle(listIndex, ccAdverts.Status_OnHold ) = styListCellNotSelected
		    
		    ccAdverts(Self.Parent).lstAdverts.Cell(listIndex, ccAdverts.Status_NotPlaced) = ""
		    ccAdverts(Self.Parent).lstAdverts.Cell(listIndex, ccAdverts.Status_Placed) = ""
		    ccAdverts(Self.Parent).lstAdverts.Cell(listIndex, ccAdverts.Status_Sold ) = ""
		    ccAdverts(Self.Parent).lstAdverts.Cell(listIndex, ccAdverts.Status_Deleted ) = ""
		    
		    
		    Select Case cboAdvertStatus.RowTag(cboAdvertStatus.ListIndex)
		      
		    Case "CR", "NP"
		      
		      ccAdverts(Self.Parent).lstAdverts.Cell(listIndex, ccAdverts.Status_NotPlaced ) = "NotPlaced"
		      ccAdverts(Self.Parent).lstAdverts.CellStyle(listIndex, ccAdverts.Status_NotPlaced ) = styListCellSelected
		      
		    Case "PL"
		      ccAdverts(Self.Parent).lstAdverts.Cell(listIndex, ccAdverts.Status_Placed ) = "Placed"
		      ccAdverts(Self.Parent).lstAdverts.CellStyle(listIndex, ccAdverts.Status_Placed  ) = styListCellSelected
		      
		    Case "SL"
		      ccAdverts(Self.Parent).lstAdverts.Cell(listIndex, ccAdverts.Status_Sold ) = "Sold"
		      ccAdverts(Self.Parent).lstAdverts.CellStyle(listIndex, ccAdverts.Status_Sold) = styListCellSelected
		      
		    Case "DL"
		      ccAdverts(Self.Parent).lstAdverts.Cell(listIndex, ccAdverts.Status_Deleted ) = "Deleted"
		      ccAdverts(Self.Parent).lstAdverts.CellStyle(listIndex, ccAdverts.Status_Deleted ) = styListCellSelected
		      
		      
		    Case Else
		      
		    End Select
		    
		    
		  Catch Err as RunTimeException
		    session.DisplayError "ccAdvert.UpdateAdvertList", Err
		    
		  End Try
		  
		  
		End Sub
