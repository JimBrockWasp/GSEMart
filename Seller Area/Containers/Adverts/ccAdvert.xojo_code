	#tag Method, Flags = &h0
		Sub LoadAdvert()
		  ' ----------------------------------------------------------------------
		  ' Procedure Name: ccAdvert.LoadAdvert
		  ' Date          :
		  ' By            : Jim Brock
		  ' Notes
		  '
		  ' ----------------------------------------------------------------------
		  Try
		    
		    ' Connect to the database
		    Dim db As New MSSQLServerDatabaseEx
		    Dim sp As New SQLStoredProcedure(db, "prSelAdvertByID")
		    Dim rs As RecordSet
		    
		    sp.AddParameter("@AdvertId", AdvertId)
		    rs = sp.ExecuteRecordSet
		    
		    txtAdvertId.Text = CStr(AdvertId)
		    txtGSEType.Text = rs.Field("GSETypeDescription").StringValue.DefineEncoding(Encodings.WindowsANSI)
		    GSETypeId = rs.Field("GSETypeId").IntegerValue
		    
		    
		    cboAdvertStatus.SetSelected(rs.Field("AdvertStatusId").StringValue.DefineEncoding(Encodings.WindowsANSI))
		    CurrentAdvertStatusId = rs.Field("AdvertStatusId").StringValue.DefineEncoding(Encodings.WindowsANSI)
		    
		    cboAvailability.SetSelected(rs.Field("AdvertAvailabilityId").StringValue.DefineEncoding(Encodings.WindowsANSI))
		    cboCountry.SetSelected(rs.Field("CountryId").StringValue.DefineEncoding(Encodings.WindowsANSI))
		    cboManafacturer.SetSelected(rs.Field("Manufacturer").StringValue.DefineEncoding(Encodings.WindowsANSI))
		    
		    
		    txtTitle.Text = rs.Field("Title").StringValue.DefineEncoding(Encodings.WindowsANSI)
		    txtLocation.Text = rs.Field("Location").StringValue.DefineEncoding(Encodings.WindowsANSI)
		    txtLongDescription.Text = rs.Field("LongDescription").StringValue.DefineEncoding(Encodings.WindowsANSI)
		    txtQuantity.Text = rs.Field("Quantity").StringValue.DefineEncoding(Encodings.WindowsANSI)
		    txtShortDescription.Text = rs.Field("ShortDescription").StringValue.DefineEncoding(Encodings.WindowsANSI)
		    txtTelephone.Text = rs.Field("Telephone").StringValue.DefineEncoding(Encodings.WindowsANSI)
		    txtURL.Text = rs.Field("URL").StringValue.DefineEncoding(Encodings.WindowsANSI)
		    txtContactName.Text = rs.Field("ContactName").StringValue.DefineEncoding(Encodings.WindowsANSI)
		    txtEmail.Text = rs.Field("EmailAddress").StringValue.DefineEncoding(Encodings.WindowsANSI)
		    txtYear.Text = rs.Field("Year").StringValue.DefineEncoding(Encodings.WindowsANSI)
		    
		    
		    chkForLease.Value = rs.Field("ForLeaseYN").BooleanValue
		    chkForRent.Value = rs.Field("ForRentYN").BooleanValue
		    chkForSale.Value = rs.Field("ForSaleYN").BooleanValue
		    
		    chkShowPrice.Value = rs.Field("ShowPriceYN").BooleanValue
		    
		    If rs.Field("ShowPriceYN").BooleanValue = True Then
		      txtPrice.Text = rs.Field("Price").StringValue.DefineEncoding(Encodings.WindowsANSI)
		      txtPrice.Enabled = True
		      cboCurrency.Enabled = True
		      cboCurrency.SetSelected(rs.Field("CurrencyId").StringValue.DefineEncoding(Encodings.WindowsANSI))
		      
		    Else
		      txtPrice.Text = "POA"
		      txtPrice.Enabled = False
		      cboCurrency.Enabled = False
		      
		    End If
		    
		    If rs.Field("NewYN").BooleanValue = True Then
		      rdoNewUsed.CellValue(0,0) = True
		    Else
		      rdoNewUsed.CellValue(0,1) = True
		    End If
		    
		    rs.close
		    
		    ' Check whether this user can advertise new equipment
		    If Session.User.NewYN = "Yes" Then
		      rdoNewUsed.Enabled = True
		    Else
		      rdoNewUsed.Enabled = False
		    End If
		    
		    
		    
		    ' Disconnect
		    db.Close
		    
		  Catch Err as RunTimeException
		    Session.DisplayError "ccAdvert.LoadAdvert", Err
		    
		  End Try
		End Sub
	#tag EndMethod

