		Sub Shown()
		  ' ----------------------------------------------------------------------
		  ' Procedure Name: ccAdvert.Shown
		  ' Date          :
		  ' By            : Jim Brock
		  ' Notes
		  '
		  ' ----------------------------------------------------------------------
		  Try
		    
		    ccAdverts(Me.Parent).conAddNew.Top = -1000
		    
		    If LookupsLoadedYN = False Then
		      
		      LoadAvailability
		      LoadCountry
		      LoadCurrency
		      LoadAdvertStatus
		      LoadManufacturers
		      
		      ' Lookups are now loaded
		      LookupsLoadedYN = True
		      
		      
		    End If
		    
