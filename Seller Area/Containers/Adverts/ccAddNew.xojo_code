	#tag Method, Flags = &h0
		Function ValidateEntries() As Boolean
		  ' ----------------------------------------------------------------------
		  ' Procedure Name: ccAddNew.ValidateEntries
		  ' By            : Jim Brock
		  ' Notes
		  '
		  ' ----------------------------------------------------------------------
		  Try
		    
		    
		    ' Check that the values have been entered
		    Dim Messages() As String
		    
		    If cboGSETypes.ListIndex = -1 Then
		      Messages.Append ("* GSE Type")
		    End If
		    
		    If txtTitle.Text = "" Then
		      Messages.Append("* Title")
		    End If
		    
		    If txtShortDescription.Text = "" Then
		      Messages.Append("* Short Description")
		    End If
		    
		    If txtLongDescription.Text = "" Then
		      Messages.Append("* Long Description")
		    End If
		    
		    If cboManufacturer.ListIndex = -1 Then
		      Messages.Append ("* Manufacturer")
		    End If
		    
		    If cboCountry.ListIndex = -1 Then
		      Messages.Append ("* Country")
		    End If
		    
		    If chkShowPrice.Value = True And cboCurrency.ListIndex = -1 Then
		      Messages.Append ("* Currency")
		    End If
		    
		    If chkForLease.Value = False And chkForRent.Value = False and chkForSale.Value = False Then
		      Messages.Append("* For Sale, Rent or Lease")
		    End If
		    
		    If cboAvailability.ListIndex = -1 Then
		      Messages.Append ("* Availability")
		    End If
		    
		    If Messages.Ubound > -1 Then
		      
		      Dim dlgMessage As New dlgErrorMessage
		      dlgMessage.lblHeading.Text = "Invalid Entry"
		      dlgMessage.lblMessage.Text = "Please enter the following values: " + EndOfLine + EndOfLine + Join(Messages, EndOfLine)
		      dlgMessage.Show
		      
		      Return False
		      
		    Else
		      
		      Return True
		      
		    End If
		    
		  Catch Err as RunTimeException
		    session.DisplayError "ccAddNew.ValidateEntries", Err
		    
		  End Try
		End Function
	#tag EndMethod


