		Sub ValueChanged()
		  ' ----------------------------------------------------------------------
		  ' Procedure Name: ccAddNew.ValueChanged
		  ' By            : Jim Brock
		  ' Notes
		  '
		  ' ----------------------------------------------------------------------
		  Try
		    
		    txtPrice.Enabled = Me.Value
		    cboCurrency.Enabled = Me.Value
		    
		    If  Me.Value = False Then
		      
		      txtPrice.Text = ""
		      cboCurrency.ListIndex = -1
		      
		    End If
		    
		    
		    
		  Catch Err as RunTimeException
		    session.DisplayError "ccAddNew.ValueChanged", Err
		    
		  End Try
		End Sub
