#tag Events cmdSave
	#tag Event
		Sub Action()
		  ' ----------------------------------------------------------------------
		  ' Procedure Name: ccAddAdvert.cmdSave.Action
		  ' Date          :
		  ' By            : Jim Brock
		  ' Notes
		  '
		  ' ----------------------------------------------------------------------
		  Try
		    
		    ' Connect to the database
		    Dim ThisAdvertId As Integer
		    Dim LastIndex As Integer
		    
		    ' Check that everything has been entered correctly
		    If ValidateEntries() = True Then
		      
		      ' Save the advert and get the AdvertId
		      ThisAdvertId = Save()
		      
		      SendNewEmail (ThisAdvertId)
		      
		      
		      ' Add a new row to the list
		      ccAdverts(Me.Parent.Parent).lstAdverts.InsertRow(0,"")
		      
		      LastIndex = 0
		      
		      ccAdverts(Me.Parent.Parent).lstAdverts.Cell(LastIndex, 0 ) = CStr(ThisAdvertId)
		      ccAdverts(Me.Parent.Parent).lstAdverts.Cell(LastIndex, 1 ) = cboGSETypes.List(cboGSETypes.ListIndex)
		      ccAdverts(Me.Parent.Parent).lstAdverts.Cell(LastIndex, 2 ) = txtTitle.Text
		      
		      ccAdverts(Me.Parent.Parent).lstAdverts.CellStyle(LastIndex, 0 ) = styListId
		      
		      ccAdverts(Me.Parent.Parent).lstAdverts.CellStyle(LastIndex, ccAdverts.Status_NotPlaced ) = styListCellSelected
		      ccAdverts(Me.Parent.Parent).lstAdverts.Cell(LastIndex, ccAdverts.Status_NotPlaced ) = "Not Placed"
		      
		      ccAdverts(Me.Parent.Parent).lstAdverts.CellStyle(LastIndex, ccAdverts.Status_Placed) = styListCellNotSelected
		      ccAdverts(Me.Parent.Parent).lstAdverts.CellStyle(LastIndex, ccAdverts.Status_Sold) = styListCellNotSelected
		      ccAdverts(Me.Parent.Parent).lstAdverts.CellStyle(LastIndex, ccAdverts.Status_Deleted) = styListCellNotSelected
		      ccAdverts(Me.Parent.Parent).lstAdverts.CellStyle(LastIndex, ccAdverts.Status_OnHold) = styListCellNotSelected
		      
		      
		      ' Hide the add new form
		      Me.Parent.Visible = False
		      
		      ccAdverts(Me.Parent.Parent).RowClicked = 0
		      ccAdverts(Me.Parent.Parent).SelectedAdvertId = ThisAdvertId
		      ccAdverts(Me.Parent.Parent).NavigateTo("Advert")
		      
		    End If
		    
		  Catch Err as RunTimeException
		    Session.DisplayError "ccAddAdvert.cmdSave.Action", Err
		    
		  End Try
		End Sub
	#tag EndEvent
#tag EndEvents
