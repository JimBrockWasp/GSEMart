#tag WindowCode
	#tag Event
		Sub Shown()
		  ' ----------------------------------------------------------------------
		  ' Procedure Name: ccAddNew.Shown
		  ' Date          :
		  ' By            : Jim Brock
		  ' Notes
		  '
		  ' ----------------------------------------------------------------------
		  Try
		    
		    If LookupsLoadedYN = False Then
		      
		      LoadAvailability
		      LoadManufacturers
		      LoadGSETypes
		      LoadCurrency
		      LoadCountry
		      
		      ' Lookups are now loaded
		      LookupsLoadedYN = True
		      
		      
		    End If
		    
		  Catch Err as RunTimeException
		    Session.DisplayError "ccAddNew.Shown", Err
		    
		  End Try
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub LoadAvailability()
		  ' ----------------------------------------------------------------------
		  ' Procedure Name: LoadAvailability
		  ' Date          :
		  ' By            : Jim Brock
		  ' Notes
		  '
		  ' ----------------------------------------------------------------------
		  Try
		    
		    ' Connect to the database
		    Dim db As New MSSQLServerDatabaseEx
		    Dim sp As New SQLStoredProcedure(db, "prSelAdvertAvailability")
		    Dim rs As RecordSet
		    
		    ' Advert Status
		    rs = sp.ExecuteRecordSet
		    cboAvailability.Load(rs)
		    rs.Close
		    
		    db.Close
		    
		    
		  Catch Err as RunTimeException
		    session.DisplayError "LoadAvailability", Err
		    
		  End Try
		End Sub
	#tag EndMethod

