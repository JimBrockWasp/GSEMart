		Function Save() As Integer
		  ' ----------------------------------------------------------------------
		  ' Procedure Name: ccAddNew.Save
		  ' By            : Jim Brock
		  ' Notes
		  '
		  ' ----------------------------------------------------------------------
		  
		  Dim ThisAdvertId As Integer
		  
		  
		  ' Connect to the database
		  Dim db As New MSSQLServerDatabaseEx
		  Dim sp As New SQLStoredProcedure(db, "slAddBlankAdvert")
		  Dim rs As RecordSet
		  
		  sp.AddParameter("@AccountId", Session.User.AccountId)
		  sp.AddParameter("@GSETypeId", cboGSETypes.RowTag(cboGSETypes.ListIndex).IntegerValue)
		  
		  sp.AddParameter("@Title", txtTitle.Text)
		  sp.AddParameter("@ShortDescription", txtShortDescription.Text)
		  sp.AddParameter("@LongDescription", txtLongDescription.Text)
		  sp.AddParameter("@ContactName", txtContactName.Text)
		  sp.AddParameter("@EmailAddress", txtEmail.Text)
		  sp.AddParameter("@Telephone", txtTelephone.Text)
		  sp.AddParameter("@Location", txtLocation.Text)
		  sp.AddParameter("@URL", txtURL.Text)
		  sp.AddParameter("@Year", txtYear.Text)
		  
		  sp.AddParameter("@ForLeaseYN", chkForLease.Value)
		  sp.AddParameter("@ForSaleYN", chkForSale.Value)
		  sp.AddParameter("@ForRentYN", chkForRent.Value)
		  
		  sp.AddParameter("@NewYN", rdoNewUsed.CellValue(0,0))
		  sp.AddParameter("@AdvertAvailabilityId", cboAvailability.RowTag(cboAvailability.ListIndex).StringValue)
		  sp.AddParameter("@Manufacturer", cboManufacturer.RowTag(cboManufacturer.ListIndex).StringValue)
		  
		  sp.AddParameter("@ShowPriceYN", chkShowPrice.Value)
		  If chkShowPrice.Value = True Then
		    sp.AddParameter("@Price", CDouble(txtPrice.Text))
		    sp.AddParameter("@CurrencyId", cboCurrency.RowTag(cboCurrency.ListIndex).StringValue)
		  End If
		  
		  sp.AddParameter("@CountryId", cboCountry.RowTag(cboCountry.ListIndex).StringValue)
		  
		  
		  rs = sp.ExecuteRecordSet
		  
		  ThisAdvertId = rs.Field("AdvertId").IntegerValue
		  rs.Close
		  
		  ' Disconnect
		  db.Close
		  
		  Dim dlgMessage As New dlgMessage
		  Dim Message() As String
		  
		  Message.Append("")
		  Message.Append("Advert created as AdvertId:" + CStr(ThisAdvertId))
		  Message.Append("")
		  Message.Append("Improve your advert by adding: ")
		  Message.Append("")
		  Message.Append("* Images")
		  Message.Append("* Technical Data")
		  Message.Append("* Documents")
		  Message.Append("")
		  Message.Append("When you are ready, 'Place' your advert.")
		  
		  dlgMessage.lblHeading.Text = "New Advert: "+ CStr(ThisAdvertId)
		  dlgMessage.lblMessage.Text = Join(Message, EndOfLine)
		  dlgMessage.Show
		  
		  
		  
		  Return ThisAdvertId
		  
		  
		End Function
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub SendNewEmail(AdvertId As Integer)
		  ' ----------------------------------------------------------------------
		  ' Procedure Name: SendNewEmail
		  ' By            : Jim Brock
		  ' Notes
		  ' Send an email to yourself to let you know when someone adds an advert
		  ' ----------------------------------------------------------------------
		  
		  
		  Dim Message As New EmailMessage
		  Dim HTML() As String
		  
		  Try
		    
		    HTML.Append("New Advert Added")
		    HTML.Append("<table>")
		    
		    HTML.Append("<tr>")
		    HTML.Append("<td>")
		    HTML.Append("AccountId:")
		    HTML.Append("</td>")
		    HTML.Append("<td>")
		    HTML.Append(CStr(Session.User.AccountId))
		    HTML.Append("</td>")
		    HTML.Append("</tr>")
		    
		    HTML.Append("<tr>")
		    HTML.Append("<td>")
		    HTML.Append("Company Name:")
		    HTML.Append("</td>")
		    HTML.Append("<td>")
		    HTML.Append(Session.User.CompanyName)
		    HTML.Append("</td>")
		    HTML.Append("</tr>")
		    
		    HTML.Append("<tr>")
		    HTML.Append("<td>")
		    HTML.Append("AdvertId")
		    HTML.Append("</td>")
		    HTML.Append("<td>")
		    HTML.Append(CStr(AdvertId))
		    HTML.Append("</td>")
		    HTML.Append("</tr>")
		    
		    HTML.Append("<tr>")
		    HTML.Append("<td>")
		    HTML.Append("Title:")
		    HTML.Append("</td>")
		    HTML.Append("<td>")
		    HTML.Append(txtTitle.Text)
		    HTML.Append("</td>")
		    HTML.Append("</tr>")
		    
		    HTML.Append("</table>")
		    
		    Message.FromAddress = Session.ConfigurationItems.Item.Value("BCCEmailAddress")
		    Message.AddRecipient(Session.ConfigurationItems.Item.Value("BCCEmailAddress"))
		    Message.Subject     = "New Advert Saved"
		    Message.BodyHTML    = Join (HTML, "")
		    
		    Session.SMTP.MailSocket.Messages.Append(Message)
		    Session.SMTP.MailSocket.SendMail
		    
		  Catch Err as RunTimeException
		    
		    ' Ignore any errors in this routine
		    'session.DisplayError "routinename", Err
		    
		  End Try
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub Show()
		  Me.Left = 0
		  Me.Top = 0
		  Me.Width = Me.Parent.Width - 10
		  Me.Height = Me.Parent.Height - 10
		  Me.Visible = True
		  
		  cboGSETypes.ListIndex = -1
		  cboManufacturer.ListIndex = -1
		  cboAvailability.ListIndex = 0
		  txtLongDescription.Text = ""
		  txtShortDescription.Text = ""
		  txtTitle.Text = ""
		  chkForSale.Value = True
		  chkForRent.Value = False
		  chkForLease.Value = False
		  rdoNewUsed.CellValue(0,1) = True
		  
		  chkShowPrice.Value = False
		  cboCurrency.Enabled = False
		  txtPrice.Text = ""
		  txtPrice.Enabled = False
		  txtLocation.Text = ""
		  txtQuantity.Text = "1"
		  
		  txtEmail.Text = Session.User.EmailAddress
		  txtURL.Text = Session.User.URL
		  txtTelephone.Text = Session.User.Telephone
		  txtContactName.Text = Session.User.ContactName
		  
		  ' Check whether this user can advertise new equipment
		  If Session.User.NewYN = "Yes" Then
		    rdoNewUsed.Enabled = True
		  Else
		    rdoNewUsed.Enabled = False
		  End If
		  
		End Sub
